{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://13.48.56.255:5000/:path*"
    }
  ],
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
